{
  "key": "html-utils",
  "public": true,
  "documentationUrl": "https://prismatic.io/docs/components/html-utils/",
  "display": {
    "label": "HTML Utils",
    "description": "Helpful HTML-related functions for building HTML documents and HTML-based emails.",
    "iconPath": "icon.png",
    "category": "Helpers",
    "iconBase64": ""
  },
  "connections": [],
  "actions": {
    "convertInlineCss": {
      "display": {
        "label": "Convert to Inline CSS",
        "description": "Convert an HTML document to use inline styles"
      },
      "examplePayload": {
        "data": "<html>\n  <head>\n    <title>My Webpage</title>\n  </head>\n  <body>\n    <div style=\"color: red;\">Hello World</div>\n  </body>\n</html>\n"
      },
      "key": "convertInlineCss",
      "inputs": [
        {
          "language": "html",
          "example": "<html>\n<head>\n   <title>My Webpage</title>\n   <style>div{color:red;}</style>\n</head>\n<body>\n   <div>Hello World</div>\n</body>\n</html>",
          "key": "html",
          "type": "code",
          "default": "",
          "label": "HTML"
        }
      ],
      "documentation": {
        "description": "This action leverages the [juice](https://www.npmjs.com/package/juice) NPM package to apply CSS defined in `<style>` tags to inline HTML elements.\n"
      }
    },
    "formatHtml": {
      "display": {
        "label": "Format HTML",
        "description": "Apply prettier to an HTML document"
      },
      "examplePayload": {
        "data": "<html>\n  <head>\n    <title>My Webpage</title>\n    <style>\n      div{color:red;}\n    </style>\n  </head>\n  <body>\n    <div>Hello World</div>\n  </body>\n</html>\n"
      },
      "key": "formatHtml",
      "inputs": [
        {
          "language": "html",
          "example": "<html>\n<head>\n   <title>My Webpage</title>\n   <style>div{color:red;}</style>\n</head>\n<body>\n   <div>Hello World</div>\n</body>\n</html>",
          "key": "html",
          "type": "code",
          "default": "",
          "label": "HTML"
        }
      ]
    },
    "stripHtmlTags": {
      "display": {
        "label": "Strip HTML Tags",
        "description": "Remove all HTML tags from a string"
      },
      "key": "stripHtmlTags",
      "inputs": [
        {
          "language": "html",
          "example": "<html>\n<head>\n   <title>My Webpage</title>\n   <style>div{color:red;}</style>\n</head>\n<body>\n   <div>Hello World</div>\n</body>\n</html>",
          "key": "html",
          "type": "code",
          "default": "",
          "label": "HTML"
        }
      ]
    }
  },
  "triggers": {},
  "dataSources": {},
  "documentation": {
    "description": "The **HTML Utils** component provides helpful functions for building and formatting HTML documents.\n"
  },
  "$version": 1
}
