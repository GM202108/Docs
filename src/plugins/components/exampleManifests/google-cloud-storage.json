    "listFiles": {
      "display": {
        "label": "List Files",
        "description": "List files in a Google Cloud Storage bucket"
      },
      "inputs": [
        {
          "label": "Connection",
          "type": "connection",
          "required": true,
          "key": "connection"
        },
        {
          "label": "Bucket Name",
          "placeholder": "Bucket Name",
          "type": "string",
          "required": true,
