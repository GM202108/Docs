  "connections": [
    {
      "key": "privateKey",
      "label": "Google Cloud Storage Private Key",
      "comments": "Authenticate requests to Google Cloud Storage using values obtained from the Google Cloud Platform.",
      "inputs": [
        {
          "label": "Client Email",
          "placeholder": "Client Email",
          "type": "string",
          "required": true,
          "example": "someone@example.com",
          "shown": true,
          "comments": "The email address of the client you would like to connect.",
          "key": "clientEmail"
        },
        {
          "label": "Private Key",
          "placeholder": "Private Key",
          "type": "text",
          "required": true,
          "shown": true,
          "comments": "The private key of the client you would like to connect.",
          "key": "privateKey"
        },
        {
          "label": "Project Id",
          "placeholder": "Project Id",
          "type": "string",
          "required": true,
          "shown": true,
          "comments": "The ID of the project that hosts the storage bucket",
          "key": "projectId"
        }
      ],
      "documentation": {
        "description": "The Google Cloud Storage component requires a username / private key pair for authentication.\nTo create a GCP service account, follow directions on https://cloud.google.com/docs/authentication/getting-started.\n\nBe sure to assign the user you just created with the correct permissions.\nTo access Google Cloud Storage resources, you will need to assign either a resource specific permission, or the Cloud Storage Admin role.\nAfter creating a service account, you will have an option to create a key for the account.\nIf you are not greeted with this option, click on the service account you just created, and navigate to the 'keys' section.\nHere you can click 'Add Key'\nThis will generate a .json file with **type**, **project_id**, and a dozen other fields.\nUse these fields along with your GCP Project Id to create a new connection.\n"
      }
    }
  ],
  "triggers": {},
  "documentation": {
    "description": "[Google Cloud Storage](https://cloud.google.com/storage) is Google's cloud file/blob storage system.\nThis component allows you to create, download, modify, list, and delete files stored in a Google Cloud Storage bucket.\n",
    "dev": "Documentation for the Node.js client used in this component can be found at https://googleapis.dev/nodejs/storage/latest.\n\nA common integration pattern involves listing files in a file store, and performing a series of actions on the array of files that are returned.\nSee our [looping over files](quickstarts/looping-over-files.mdx) quickstart for information about how to create a loop over an array of files.\n"
  },
