{
  "key": "sftp",
  "documentationUrl": "https://prismatic.io/docs/components/sftp/",
  "public": true,
  "display": {
    "label": "SFTP",
    "description": "Read, write, move and delete files on an SFTP server",
    "iconPath": "icon.png",
    "category": "Data Platforms",
    "iconBase64": ""
  },
  "connections": [
    {
      "key": "basic",
      "label": "Basic Username/Password",
      "comments": "Basic Username and Password connection",
      "inputs": [
        {
          "placeholder": "Password",
          "required": true,
          "example": "p@s$W0Rd",
          "key": "password",
          "type": "password",
          "default": "",
          "label": "Password"
        },
        {
          "placeholder": "Username",
          "required": true,
          "example": "john.doe",
          "key": "username",
          "type": "string",
          "default": "",
          "label": "Username"
        },
        {
          "placeholder": "Name of the host",
          "required": true,
          "comments": "The address of the SFTP server. This should be either an IP address or hostname.",
          "example": "sftp.prismatic.io",
          "key": "host",
          "type": "string",
          "default": "",
          "label": "Host"
        },
        {
          "placeholder": "Port of the host",
          "required": true,
          "comments": "The port of the SFTP server.",
          "example": "2222",
          "key": "port",
          "type": "string",
          "default": "22",
          "label": "Port"
        },
        {
          "placeholder": "Timeout",
          "required": false,
          "comments": "How long the client will await a request.",
          "example": "4000",
          "key": "timeout",
          "type": "string",
          "default": "3000",
          "label": "Timeout"
        }
      ],
      "documentation": {
        "description": "The **basic auth** connection is used to connect to SFTP servers that support username / password login.\nConsult your SFTP server administrator to determine which login method the server supports.\n"
      }
    },
    {
      "key": "privateKey",
      "label": "Private Key",
      "comments": "Private key connection",
      "inputs": [
        {
          "placeholder": "SSH Private Key",
          "required": true,
          "comments": "SSH private key",
          "example": "-----BEGIN OPENSSH PRIVATE KEY-----\nabc123...",
          "key": "privateKey",
          "type": "text",
          "default": "",
          "label": "Private Key"
        },
        {
          "placeholder": "Username",
          "required": true,
          "example": "john.doe",
          "key": "username",
          "type": "string",
          "default": "",
          "label": "Username"
        },
        {
          "placeholder": "Name of the host",
          "required": true,
          "comments": "The address of the SFTP server. This should be either an IP address or hostname.",
          "example": "sftp.prismatic.io",
          "key": "host",
          "type": "string",
          "default": "",
          "label": "Host"
        },
        {
          "placeholder": "Port of the host",
          "required": true,
          "comments": "The port of the SFTP server.",
          "example": "2222",
          "key": "port",
          "type": "string",
          "default": "22",
          "label": "Port"
        },
        {
          "placeholder": "Timeout",
          "required": false,
          "comments": "How long the client will await a request.",
          "example": "4000",
          "key": "timeout",
          "type": "string",
          "default": "3000",
          "label": "Timeout"
        }
      ],
      "documentation": {
        "description": "The **private key** connection allows you to access an SFTP server via SSH [public/private key authentication](https://www.ssh.com/academy/ssh/public-key-authentication).\nYou will need to generate a public/private key pair, and ensure that your public key is stored on the SFTP server that you are connecting to.\nThen, you can authenticate with the SFTP server using a username and corresponding private key.\n"
      }
    }
  ],
  "actions": {
    "readFile": {
      "display": {
        "label": "Read File",
        "description": "Read a file from SFTP"
      },
      "examplePayload": {
        "data": "Sample file contents",
        "contentType": "text/plain"
      },
      "key": "readFile",
      "inputs": [
        {
          "required": true,
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "placeholder": "Path on SFTP server to read data from",
          "required": true,
          "comments": "Path of file on SFTP server to read data from",
          "example": "/path/to/file.txt",
          "key": "inputPath",
          "type": "string",
          "default": "",
          "label": "Path"
        },
        {
          "comments": "Show additional debug output in logs. Note: SFTP is very verbose - expect hundreds of debug lines when this is enabled.",
          "required": true,
          "key": "debug",
          "type": "boolean",
          "default": "false",
          "label": "Show Debug Output"
        }
      ]
    },
    "statFile": {
      "display": {
        "label": "Stat File",
        "description": "Pull statistics about a file"
      },
      "examplePayload": {
        "data": {
          "mode": 33279,
          "uid": 1000,
          "gid": 985,
          "size": 5,
          "accessTime": 1566868566000,
          "modifyTime": 1566868566000,
          "isDirectory": false,
          "isFile": true,
          "isBlockDevice": false,
          "isCharacterDevice": false,
          "isSymbolicLink": false,
          "isFIFO": false,
          "isSocket": false
        }
      },
      "key": "statFile",
      "inputs": [
        {
          "required": true,
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "placeholder": "Path on SFTP server to read data from",
          "required": true,
          "comments": "Path of file on SFTP server to read data from",
          "example": "/path/to/file.txt",
          "key": "inputPath",
          "type": "string",
          "default": "",
          "label": "Path"
        },
        {
          "comments": "Show additional debug output in logs. Note: SFTP is very verbose - expect hundreds of debug lines when this is enabled.",
          "required": true,
          "key": "debug",
          "type": "boolean",
          "default": "false",
          "label": "Show Debug Output"
        }
      ]
    },
    "writeFile": {
      "display": {
        "label": "Write File",
        "description": "Write a file to SFTP"
      },
      "examplePayload": {
        "data": "Uploaded data stream to /upload/path/to/file.txt"
      },
      "key": "writeFile",
      "inputs": [
        {
          "required": true,
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "placeholder": "Path on SFTP server to write file",
          "required": true,
          "comments": "Path on SFTP server to write file",
          "example": "/we/love/commas.csv",
          "key": "outputPath",
          "type": "string",
          "default": "",
          "label": "Path"
        },
        {
          "placeholder": "Data to write",
          "required": true,
          "comments": "Text to write into the file",
          "key": "data",
          "type": "text",
          "default": "",
          "label": "Data"
        },
        {
          "comments": "Show additional debug output in logs. Note: SFTP is very verbose - expect hundreds of debug lines when this is enabled.",
          "required": true,
          "key": "debug",
          "type": "boolean",
          "default": "false",
          "label": "Show Debug Output"
        }
      ]
    },
    "moveFile": {
      "display": {
        "label": "Move File",
        "description": "Move a file on an SFTP server"
      },
      "key": "moveFile",
      "inputs": [
        {
          "required": true,
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "placeholder": "Path of file to move",
          "required": true,
          "comments": "Path of file to move",
          "example": "/my/starting/path.txt",
          "key": "sourcePath",
          "type": "string",
          "default": "",
          "label": "Source Path"
        },
        {
          "placeholder": "Path to move the file to",
          "required": true,
          "comments": "Path of file to move",
          "example": "/my/destination/path.txt",
          "key": "destinationPath",
          "type": "string",
          "default": "",
          "label": "Destination Path"
        },
        {
          "comments": "Show additional debug output in logs. Note: SFTP is very verbose - expect hundreds of debug lines when this is enabled.",
          "required": true,
          "key": "debug",
          "type": "boolean",
          "default": "false",
          "label": "Show Debug Output"
        }
      ]
    },
    "deleteFile": {
      "display": {
        "label": "Delete File",
        "description": "Delete a file from a SFTP server"
      },
      "key": "deleteFile",
      "inputs": [
        {
          "required": true,
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "placeholder": "Path of file to delete",
          "required": true,
          "comments": "Path of file to delete",
          "example": "/path/to/file.txt",
          "key": "path",
          "type": "string",
          "default": "",
          "label": "Path"
        },
        {
          "comments": "Show additional debug output in logs. Note: SFTP is very verbose - expect hundreds of debug lines when this is enabled.",
          "required": true,
          "key": "debug",
          "type": "boolean",
          "default": "false",
          "label": "Show Debug Output"
        }
      ]
    },
    "listDirectory": {
      "display": {
        "label": "List Directory",
        "description": "List files in a directory on an SFTP server"
      },
      "examplePayload": {
        "data": [
          "file.txt",
          "example.txt"
        ]
      },
      "key": "listDirectory",
      "inputs": [
        {
          "required": true,
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "placeholder": "Path of directory on an SFTP server to list files of",
          "required": true,
          "comments": "Path of directory on an SFTP server to list files of",
          "example": "/path/to/directory/",
          "key": "path",
          "type": "string",
          "default": "",
          "label": "Path"
        },
        {
          "placeholder": "Glob-style string for listing specific files",
          "required": false,
          "comments": "Glob-style string for listing specific files",
          "example": "*.txt",
          "key": "pattern",
          "type": "string",
          "default": "*",
          "label": "Pattern"
        },
        {
          "comments": "Show additional debug output in logs. Note: SFTP is very verbose - expect hundreds of debug lines when this is enabled.",
          "required": true,
          "key": "debug",
          "type": "boolean",
          "default": "false",
          "label": "Show Debug Output"
        }
      ]
    }
  },
  "triggers": {},
  "documentation": {
    "description": "The SFTP (_SSH File Transfer Protocol_) component lets you upload, download, move and delete files on an SFTP server.\n",
    "dev": "A common integration pattern involves listing files in a file store, and performing a series of actions on the array of files that are returned.\nSee our [looping over files](quickstarts/looping-over-files.mdx) quickstart for information about how to create a loop over an array of files.\n"
  },
  "$version": 1
}
