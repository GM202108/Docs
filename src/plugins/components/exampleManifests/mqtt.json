{
  "key": "mqtt",
  "documentationUrl": "https://prismatic.io/docs/components/mqtt/",
  "public": true,
  "display": {
    "label": "MQTT",
    "category": "Application Connectors",
    "description": "Interact with an MQTT Queue",
    "iconPath": "icon.png",
    "iconBase64": ""
  },
  "actions": {
    "publish": {
      "display": {
        "label": "Publish Message",
        "description": "Publish a message to a MQTT topic."
      },
      "inputs": [
        {
          "label": "Host",
          "example": "192.168.0.1",
          "placeholder": "Host",
          "type": "string",
          "required": true,
          "comments": "An MQTT broker is a server that receives all messages from the clients and then routes the messages to the appropriate destination clients.",
          "key": "host"
        },
        {
          "label": "Port",
          "example": "1883",
          "type": "string",
          "default": "1883",
          "required": true,
          "comments": "Provide a string value for the port your broker server is exposing.",
          "key": "port"
        },
        {
          "label": "Topic Name",
          "example": "myTopic",
          "type": "string",
          "required": true,
          "comments": "Provide a string value for the name of the MQTT topic.",
          "key": "topicName"
        },
        {
          "label": "Message",
          "example": "Message to Queue",
          "placeholder": "Message",
          "type": "string",
          "required": true,
          "comments": "Provide a string value to be sent to the MQTT topic.",
          "key": "message"
        },
        {
          "label": "Connection",
          "type": "connection",
          "required": true,
          "key": "mqttConnection"
        }
      ],
      "key": "publish"
    }
  },
  "connections": [
    {
      "key": "mqtt",
      "label": "MQTT Connection",
      "comments": "Authenticate requests to an MQTT server.",
      "inputs": [
        {
          "label": "Host",
          "placeholder": "Host",
          "type": "string",
          "required": true,
          "shown": false,
          "comments": "Provide the string value for the host of the MQTT server.",
          "default": "192.168.0.1",
          "key": "host"
        },
        {
          "label": "Port",
          "placeholder": "Port",
          "type": "string",
          "required": true,
          "shown": true,
          "comments": "The port of the MQTT server.",
          "default": "1883",
          "key": "port"
        },
        {
          "label": "Username",
          "placeholder": "Username",
          "type": "string",
          "required": false,
          "shown": true,
          "key": "username"
        },
        {
          "label": "Password",
          "placeholder": "Password",
          "type": "string",
          "required": false,
          "shown": true,
          "key": "password"
        }
      ]
    }
  ],
  "triggers": {},
  "documentation": {
    "description": "[Message Queuing Telemetry Transport](https://mqtt.org/) (MQTT) is a light-weight, efficient _publish-subscribe_ network protocol for sending messages between devices.\nThis component allows you to publish messages to an MQTT queue topic.\n"
  },
  "$version": 1
}
