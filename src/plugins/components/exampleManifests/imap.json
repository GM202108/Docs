        "data": [
          {
            "path": "Drafts",
            "pathAsListed": "Drafts",
            "flags": {},
            "delimiter": "/",
            "listed": true,
            "parentPath": "",
            "parent": [],
            "name": "Drafts",
            "subscribed": true
          },
          {
            "path": "INBOX",
            "pathAsListed": "INBOX",
            "flags": {},
            "delimiter": "/",
            "listed": true,
            "specialUse": "\\Inbox",
            "parentPath": "",
            "parent": [],
            "name": "INBOX",
            "subscribed": true
          },
          {
            "path": "Sent",
            "pathAsListed": "Sent",
            "flags": {},
            "delimiter": "/",
            "listed": true,
            "parentPath": "",
            "parent": [],
            "name": "Sent",
            "subscribed": true
