{
  "key": "fluent-commerce",
  "documentationUrl": "https://prismatic.io/docs/components/fluent-commerce/",
  "public": true,
  "display": {
    "label": "Fluent Commerce",
    "description": "Manage orders within Fluent Commerce",
    "iconPath": "icon.png",
    "category": "Application Connectors",
    "iconBase64": ""
  },
  "connections": [
    {
      "key": "fluent-password-grant",
      "label": "Fluent Commerce OAuth 2.0 Password Grant",
      "comments": "Authenticate requests to Fluent Commerce using a username and password",
      "inputs": [
        {
          "required": true,
          "placeholder": "https://REPLACE-ME.api.fluentretail.com",
          "example": "https://REPLACE-ME.api.fluentretail.com",
          "comments": "The retailer's Fluent API endpoint",
          "key": "host",
          "type": "string",
          "default": "",
          "label": "Fluent Host"
        },
        {
          "required": true,
          "comments": "The retailer's username",
          "example": "john_doe",
          "key": "username",
          "type": "string",
          "default": "",
          "label": "Retailer Username"
        },
        {
          "required": true,
          "comments": "The retailer's password",
          "key": "password",
          "type": "password",
          "default": "",
          "label": "Retailer Password"
        },
        {
          "required": true,
          "comments": "A client ID obtained from Fluent support",
          "key": "clientId",
          "type": "string",
          "default": "",
          "label": "OAuth 2.0 Client ID"
        },
        {
          "required": true,
          "comments": "A client secret obtained from Fluent support",
          "key": "clientSecret",
          "type": "password",
          "default": "",
          "label": "OAuth 2.0 Client Secret"
        }
      ],
      "documentation": {
        "description": "Fluent uses OAuth 2.0 password grant type for authentication.\nTo authenticate with Fluent, you will need a **Client ID** and **Client Secret** from Fluent, and your end users will need to enter their username and password.\n"
      }
    }
  ],
  "actions": {
    "createCustomer": {
      "display": {
        "label": "Create Customer",
        "description": "Create a new customer"
      },
      "key": "createCustomer",
      "inputs": [
        {
          "required": true,
          "comments": "An OAuth 2.0 password grant type connection",
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "example": "34",
          "required": true,
          "comments": "The ID of the retailer (a number)",
          "key": "retailerId",
          "type": "string",
          "default": "",
          "label": "Retailer ID"
        },
        {
          "example": "test@example.com",
          "required": true,
          "comments": "The email address of the customer",
          "key": "customerEmail",
          "type": "string",
          "default": "",
          "label": "Customer Email"
        },
        {
          "example": "John",
          "required": true,
          "comments": "The first name of the customer (required)",
          "key": "firstName",
          "type": "string",
          "default": "",
          "label": "First Name"
        },
        {
          "example": "Doe",
          "required": false,
          "comments": "The last name of the customer",
          "key": "lastName",
          "type": "string",
          "default": "",
          "label": "Last Name"
        },
        {
          "example": "Doe",
          "required": false,
          "comments": "The customer's phone number",
          "key": "phoneNumber",
          "type": "string",
          "default": "",
          "label": "Phone Number"
        },
        {
          "example": "Australia",
          "required": false,
          "comments": "The customer's country",
          "key": "country",
          "type": "string",
          "default": "",
          "label": "Country"
        },
        {
          "example": "UTC+5",
          "required": false,
          "comments": "The customer's time zone",
          "key": "timezone",
          "type": "string",
          "default": "",
          "label": "Time Zone"
        },
        {
          "required": true,
          "key": "promotionOptIn",
          "type": "boolean",
          "default": "false",
          "label": "Promotion Opt In"
        },
        {
          "required": false,
          "comments": "Custom attributes you would like to apply to the customer",
          "key": "attributes",
          "type": "string",
          "default": "",
          "collection": "keyvaluelist",
          "label": "Attributes"
        }
      ]
    },
    "getCustomerByEmailAddress": {
      "display": {
        "label": "Get Customer by Email",
        "description": "Find a customer by their email address"
      },
      "key": "getCustomerByEmailAddress",
      "inputs": [
        {
          "required": true,
          "comments": "An OAuth 2.0 password grant type connection",
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "required": true,
          "comments": "The customer's email address",
          "key": "emailAddress",
          "type": "string",
          "default": "",
          "label": "Customer Email Address"
        }
      ]
    },
    "genericRequest": {
      "display": {
        "label": "Generic GraphQL Request",
        "description": "Issue any GraphQL query or mutation with variables"
      },
      "key": "genericRequest",
      "inputs": [
        {
          "required": true,
          "comments": "An OAuth 2.0 password grant type connection",
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "required": true,
          "language": "graphql",
          "key": "query",
          "type": "code",
          "default": "{\n  me {\n    id\n    username\n    primaryEmail\n  }\n}",
          "label": "Query or Mutation"
        },
        {
          "required": false,
          "key": "variables",
          "type": "string",
          "default": "",
          "collection": "keyvaluelist",
          "label": "Variables"
        },
        {
          "language": "json",
          "required": false,
          "key": "variablesObject",
          "type": "code",
          "default": "",
          "label": "Variables Object"
        }
      ]
    },
    "getCurrentUser": {
      "display": {
        "label": "Get Current User",
        "description": "Get information about the currently authenticated user"
      },
      "key": "getCurrentUser",
      "inputs": [
        {
          "required": true,
          "comments": "An OAuth 2.0 password grant type connection",
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        }
      ]
    },
    "createProduct": {
      "display": {
        "label": "Create Product",
        "description": "Create a new standard product"
      },
      "examplePayload": {
        "data": {
          "createStandardProduct": {
            "id": "03440122-ea39-499b-83aa-f08b2614daf3",
            "ref": "TESTPRD3"
          }
        }
      },
      "key": "createProduct",
      "inputs": [
        {
          "required": true,
          "comments": "An OAuth 2.0 password grant type connection",
          "key": "connection",
          "type": "connection",
          "label": "Connection"
        },
        {
          "required": true,
          "example": "FCRG:PC:MASTER",
          "comments": "The reference ID (ref) of an existing product catalog",
          "key": "productCatalogueRef",
          "type": "string",
          "default": "",
          "label": "Product Catalogue Ref (ID)"
        },
        {
          "required": true,
          "example": "TESTPRD",
          "comments": "The reference ID (ref) of the new product (must be unique)",
          "key": "productRef",
          "type": "string",
          "default": "",
          "label": "Product Ref ID"
        },
        {
          "required": true,
          "example": "STANDARD",
          "comments": "The type of the product",
          "key": "productType",
          "type": "string",
          "default": "STANDARD",
          "label": "Product Type"
        },
        {
          "required": true,
          "example": "Widget",
          "comments": "The name of the product (not required to be unique)",
          "key": "productName",
          "type": "string",
          "default": "",
          "label": "Product Name"
        },
        {
          "required": true,
          "example": "Widgets: the new whatsits",
          "key": "productSummary",
          "type": "string",
          "default": "",
          "label": "Product Summary / Description"
        },
        {
          "required": true,
          "example": "012345678905",
          "comments": "The global trade number of the product",
          "key": "gtin",
          "type": "string",
          "default": "",
          "label": "Global Trade Item Number (GTIN)"
        },
        {
          "required": true,
          "example": "USD",
          "comments": "Type of currency the price is in (USD, YEN, GBP, etc)",
          "key": "priceCurrency",
          "type": "string",
          "default": "",
          "label": "Price Currency"
        },
        {
          "required": true,
          "example": "1300",
          "comments": "The price of the product (must be a floating-point number)",
          "key": "priceValue",
          "type": "string",
          "default": "",
          "label": "Price Value"
        }
      ]
    }
  },
  "triggers": {},
  "dataSources": {},
  "documentation": {
    "description": "Fluent Commerce is an order management system that allows you to track customers, orders and inventory.\n",
    "dev": "Fluent Commerce offers a [GraphQL-based](https://graphql.org/) API.\nThis gives you flexibility and control over what data you send to Fluent, and what data you request back.\n\nWhile this component offers some actions that wrap common GraphQL queries and mutations, there are many more queries and mutations for which there aren't dedicated actions.\nIf you'd like to run a query or mutation that is not represented by an action, please use the [Generic Request](#genericrequest) action.\n"
  },
  "$version": 1
}
